{
  "name": "ceos",
  "version": "1.0.0",
  "description": "Copilot Enterprise Orchestration System - Enterprise-grade platform for managing GitHub Copilot deployments",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "husky install",
    "docker:build": "docker build -t ceos .",
    "docker:run": "docker run -p 3000:3000 ceos",
    "setup:dev": "node scripts/setup-dev.js",
    "setup:interactive": "node scripts/interactive-setup.js",
    "validate:github": "node scripts/validate-github-pat.js",
    "security:scan": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "snyk test",
    "copilot:quality": "eslint . --ext .ts,.js --format json > copilot-quality.json",
    "pre-commit": "npm run lint && npm run test && npm run security:scan"
  },
  "keywords": [
    "github",
    "copilot",
    "enterprise",
    "orchestration",
    "ai",
    "productivity",
    "management"
  ],
  "author": "CEOS Development Team",
  "license": "MIT",
  "dependencies": {
    "@octokit/auth-app": "^8.0.2",
    "@octokit/webhooks": "^14.1.3",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@octokit/rest": "^22.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.3",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.2",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.39.0",
    "compression": "^1.8.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^3.0.1",
    "express-rate-limit": "^8.0.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ceos.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ceos/issues"
  },
  "homepage": "https://github.com/your-org/ceos#readme",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
